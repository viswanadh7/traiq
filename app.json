{
  "expo": {
    "name": "TraiQ",
    "slug": "traiq",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/traiq-logo.png",
    "scheme": "traiq",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/traiq-logo.png",
        "backgroundImage": "./assets/images/traiq-logo.png",
        "monochromeImage": "./assets/images/traiq-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.traiq"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/traiq-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/traiq-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fc8d3bd7-701f-4149-b7c4-4d6e7c584e49"
      }
    },
    "owner": "viswanadh"
  }
}
